{
  "name": "@spera/plugin-upstash",
  "version": "0.4.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./nextjs": {
      "import": "./dist/nextjs.mjs",
      "require": "./dist/nextjs.js",
      "types": "./dist/nextjs.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "nextjs": [
        "./dist/nextjs.d.ts"
      ]
    }
  },
  "files": [
    "dist/**",
    "package.json"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsc --watch",
    "test": "eslint --ext .ts src/"
  },
  "devDependencies": {
    "@spera/core": "*",
    "@swc/core": "1.3.85",
    "@types/node": "18.17.16",
    "tsup": "7.2.0",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@upstash/qstash": "2.1.3"
  }
}
