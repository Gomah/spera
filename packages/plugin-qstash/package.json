{
  "name": "@spera/plugin-upstash",
  "version": "0.3.6",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./nextjs": {
      "import": "./dist/nextjs.mjs",
      "require": "./dist/nextjs.js",
      "types": "./dist/nextjs.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "nextjs": [
        "./dist/nextjs.d.ts"
      ]
    }
  },
  "files": [
    "dist/**",
    "package.json"
  ],
  "scripts": {
    "build": "tsup src/**/*.ts --format esm,cjs && yarn build:types",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "tsc --watch",
    "test": "eslint --ext .ts src/"
  },
  "devDependencies": {
    "@spera/core": "*",
    "@swc/core": "1.3.51",
    "@types/node": "18.15.11",
    "tsup": "6.7.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@upstash/qstash": "0.3.6"
  }
}
