{
  "name": "@spera/plugin-qstash",
  "version": "0.9.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": ">=20.x"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./nextjs": {
      "import": "./dist/nextjs.mjs",
      "require": "./dist/nextjs.js",
      "types": "./dist/nextjs.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "nextjs": [
        "./dist/nextjs.d.ts"
      ]
    }
  },
  "files": [
    "dist/**",
    "package.json"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsc --watch",
    "test": "eslint --ext .ts src/"
  },
  "devDependencies": {
    "@spera/core": "*",
    "@swc/core": "1.5.7",
    "@types/node": "20.12.13",
    "tsup": "8.0.2",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "@upstash/qstash": "2.5.4"
  }
}
