{
  "name": "@spera/plugin-upstash",
  "version": "0.3.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./nextjs": {
      "import": "./dist/nextjs.js",
      "require": "./dist/nextjs.js",
      "types": "./dist/nextjs.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "nextjs": [
        "./dist/nextjs.d.ts"
      ]
    }
  },
  "files": [
    "dist/**",
    "package.json"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "eslint --ext .ts src/"
  },
  "devDependencies": {
    "@spera/core": "*",
    "@types/node": "18.11.10",
    "typescript": "4.9.3"
  },
  "dependencies": {
    "@upstash/qstash": "0.3.1"
  }
}
